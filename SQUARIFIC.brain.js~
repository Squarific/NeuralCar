var SQUARIFIC = SQUARIFIC || {};
SQUARIFIC.brain = {};
for(var i = 1; i < net.layers.length; i++) {
    var layer = net.layers[i];
    var outputs = {};
    for(var id in layer) {
      var node = layer[id];
      var sum = node.bias;
      for(var iid in node.weights) sum += node.weights[iid] * inputs[iid];
      outputs[id] = (1/(1 + Math.exp(-sum)));
    }
    inputs = outputs;
  }
return outputs;

Neem inputs {0: 0.6, 1:0.9, 2: 0.1}

